{% extends 'tima/oai_pmh/base.xml' %}
{% load oai_pmh tima %}

{% block content %}
<responseDate>{% timestamp 'UTC' %}</responseDate>
<request {% for param in params %}{{ param }}="{{ params|lookup:param }}" {% endfor %}>{% baseurl %}</request>
{% for error in errors %}
    <error code="{{ error|lookup:'code' }}">{{ error|lookup:'msg' }}</error>
{% endfor %}
{% endblock %}
