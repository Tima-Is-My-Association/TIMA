{% extends 'tima/oai_pmh/base.xml' %}
{% load oai_pmh %}

{% block content %}
<responseDate>{% timestamp 'UTC' %}</responseDate>
<request verb="Identify">{% baseurl %}</request>
<Identify>
    <repositoryName>TIMA Repository</repositoryName>
    <baseURL>{% baseurl %}</baseURL>
    <protocolVersion>2.0</protocolVersion>
    {% adminemails %}
    <earliestDatestamp>2015-07-02T00:00:00Z</earliestDatestamp>
    <deletedRecord>no</deletedRecord>
    <granularity>YYYY-MM-DDThh:mm:ssZ</granularity>
</Identify>
{% endblock %}
